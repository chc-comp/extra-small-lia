(set-logic HORN)
(set-info :status sat)
(declare-fun itp1 (Int Int ) Bool)
(declare-fun itp2 (Int Int ) Bool)

(assert (forall ((x1 Int) (y1 Int)) (=> (and (= x1 0) (= y1 0)) (itp1 x1 y1))))
(assert (forall ((x1 Int) (y1 Int) (x2 Int) (y2 Int))
  (=> (and (itp1 x1 y1) (= x2 (+ x1 1)) (= y2 (+ y1 1))) (itp1 x2 y2))))
(assert (forall ((x1 Int) (y1 Int)) (=> (itp1 x1 y1) (itp2 x1 y1))))
(assert (forall ((x1 Int) (y1 Int) (x2 Int) (y2 Int))
  (=> (and (itp2 x1 y1) (= x2 (- x1 1)) (= y2 (- y1 1))) (itp2 x2 y2))))
(assert (forall ((x1 Int) (y1 Int))
  (let ((a!1 (and (itp2 x1 y1) (not (=> (<= x1 0) (<= y1 0))))))
    (=> a!1 false))))
(check-sat)
