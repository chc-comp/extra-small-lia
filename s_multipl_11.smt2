(set-logic HORN)
(set-info :status sat)
(declare-fun LOOPY (Int Int ) Bool)
(declare-fun LOOPZ (Int Int Int ) Bool)

(assert (forall ((y Int) (z Int)) (=> (< y z) (LOOPY y z))))
(assert (forall ((y Int) (z Int) (y1 Int) (z1 Int))
  (=> (and (LOOPY y z) (< y z) (= y1 (+ y 1000)) (= z1 z)) (LOOPY y1 z1))))
(assert (forall ((y Int) (z Int)) (=> (and (LOOPY y z) (not (< y z))) (LOOPZ 0 y z))))
(assert (forall ((x Int) (y Int) (z Int) (x1 Int) (y1 Int) (z1 Int))
  (=> (and (LOOPZ x y z) (> y z) (= x1 (+ x 1)) (= y1 y) (= z1 (+ z 2)))
      (LOOPZ x1 y1 z1))))
(assert (forall ((y Int) (z Int) (x Int))
  (=> (and (LOOPZ x y z) (not (> y z)) (not (<= x 500))) false)))
(check-sat)
