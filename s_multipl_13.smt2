(set-logic HORN)
(set-info :status sat)
(declare-fun PRE (Int Int Int Int ) Bool)
(declare-fun POST1 (Int Int Int ) Bool)
(declare-fun POST2 (Int Int Int ) Bool)

(assert (forall ((n Int)) (=> (>= n 0) (PRE n n 0 0))))
(assert (forall ((n Int) (i Int) (j Int) (n1 Int) (m Int) (i1 Int) (j1 Int))
  (let ((a!1 (or (and (= i1 (+ i 1)) (= j1 j)) (and (= i1 i) (= j1 (+ j 1))))))
    (=> (and (PRE n m i j) (not (= n 0)) (= n1 (- n 1)) a!1) (PRE n1 m i1 j1)))))
(assert (forall ((n Int) (m Int) (i Int) (j Int))
  (=> (and (PRE n m i j) (= n 0)) (POST1 m i j))))
(assert (forall ((i Int) (m Int) (m1 Int) (i1 Int) (j Int))
  (=> (and (POST1 m i j) (not (= i 0)) (= i1 (- i 1)) (= m1 (- m 1)))
      (POST1 m1 i1 j))))
(assert (forall ((m Int) (i Int) (j Int))
  (=> (and (POST1 m i j) (= i 0)) (POST2 m i j))))
(assert (forall ((j Int) (m Int) (m1 Int) (i Int) (j1 Int))
  (=> (and (POST2 m i j) (not (= j 0)) (= j1 (- j 1)) (= m1 (- m 1)))
      (POST2 m1 i j1))))
(assert (forall ((i Int) (j Int) (m Int))
  (=> (and (POST2 m i j) (= j 0) (not (= m 0))) false)))
(check-sat)
