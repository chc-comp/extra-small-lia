(set-logic HORN)
(set-info :status sat)
(declare-fun PRE (Int Int Int Int ) Bool)
(declare-fun POST (Int Int Int ) Bool)

(assert (forall ((n Int)) (=> (>= n 0) (PRE n n 0 0))))
(assert (forall ((n Int) (i Int) (j Int) (n1 Int) (m Int) (i1 Int) (j1 Int))
  (let ((a!1 (or (and (= i1 (+ i 1)) (= j1 j)) (and (= i1 i) (= j1 (+ j 1))))))
    (=> (and (PRE n m i j) (> n 0) (= n1 (- n 1)) a!1) (PRE n1 m i1 j1)))))
(assert (forall ((n Int) (m Int) (i Int) (j Int))
  (=> (and (PRE n m i j) (= n 0)) (POST m i j))))
(assert (forall ((m Int) (i Int) (j Int) (m1 Int) (i1 Int) (j1 Int))
  (let ((a!1 (or (and (= i1 (- i 1)) (= j1 j)) (and (= i1 i) (= j1 (- j 1))))))
    (=> (and (POST m i j) (> m 0) (= m1 (- m 1)) a!1) (POST m1 i1 j1)))))
(assert (forall ((i Int) (j Int) (m Int))
  (=> (and (POST m i j) (= i 0) (= j 0) (not (= m 0))) false)))
(check-sat)
