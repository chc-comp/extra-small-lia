(set-logic HORN)
(set-info :status sat)
(declare-fun itp (Int Int Int Int ) Bool)

(assert (forall ((x1 Int) (x3 Int) (x5 Int) (x7 Int))
  (=> (and (= x1 0) (= x3 0) (= x5 0) (= x7 0)) (itp x1 x3 x5 x7))))
(assert (forall ((x1 Int)
         (x3 Int)
         (x5 Int)
         (x7 Int)
         (x2 Int)
         (x4 Int)
         (x6 Int)
         (x8 Int))
  (=> (and (itp x1 x3 x5 x7)
           (= x2 (+ x1 1))
           (= x4 (+ x3 x2))
           (= x6 (+ x5 x4))
           (= x8 (+ x7 x6)))
      (itp x2 x4 x6 x8))))
(assert (forall ((x1 Int) (x3 Int) (x5 Int) (x7 Int))
  (=> (and (itp x1 x3 x5 x7) (not (>= x7 0))) false)))
(check-sat)

